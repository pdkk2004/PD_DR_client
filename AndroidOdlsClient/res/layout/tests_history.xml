<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent" android:id="@+id/LinearLayout01">
	<EditText android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:hint="Search Test" android:id="@+id/edit_search">
	</EditText>
	<ListView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/list_test" android:background="@drawable/black" android:scrollbars="vertical">
	</ListView>
	<TextView android:textAppearance="?android:attr/textAppearanceMedium"
		android:layout_height="wrap_content" android:text="No test history"
		android:id="@android:id/empty" android:layout_width="match_parent">
	</TextView>
</LinearLayout>